                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < 3; ++i) {%>
                <% if(i % 2 === 0) {%>
                <tr>
                    <% } else {%>
                <tr style="background-color: #f3f3f3;">
                    <% } %>
                    <td class="dateCell">
                        <div class="dateDay">
                            Sun
                        </div>
                        <div class="dateFormat">
                            11/5/2020</div>
                    </td>
                    <td>sometime</td>
                    <td>Othertime</td>
                    <td>24</td>
                    <td>30</td>
                    <td>24</td>
                    <td>24</td>
                    <td>24</td>
                    <td><a class="editLink" href=#>Request Edit</a></td>
                </tr>
                <% } %>
                <tr style="border-bottom-style: hidden; border-left-style: hidden;">
                    <td>
                    </td>
                    <td style="border-left-style: hidden; border-right-style: hidden;"></td>
                    <td></td>
                    <td class="totals">
                        Totals</td>
                    <td class="totals">40.00</td>
                    <td class="totals">30.00</td>
                    <td class="totals">24.00</td>
                    <td class="totals">36.00</td>
                    <td class="totals"><a href=#></a></td>
                </tr>
            </tbody>
        </table>
    </div>
</body>

</html>